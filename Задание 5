function Fizzbuzz(n) {
    for (let i = 1, mess = ""; i <= n; i++, mess = "") {
        if (i % 3 == 0) {
            mess = "fizz";
        }
        if (i % 5 == 0) {
            mess = mess + "buzz";
        }
        if (mess.length == 0) {
            mess = String(i);
        }
        console.log(mess);
    }
}

Fizzbuzz(15);
